{
    "manifest_version": 3,
    "name": "YouTube Videovorschläge ausblenden",
    "version": "1.0",
    "description": "Blendet die Videovorschläge auf YouTube aus.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }